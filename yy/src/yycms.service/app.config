<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<sectionGroup name="jsEngineSwitcher">
			<section name="core" type="JavaScriptEngineSwitcher.Core.Configuration.CoreConfiguration, JavaScriptEngineSwitcher.Core" />
			<section name="v8" type="JavaScriptEngineSwitcher.V8.Configuration.V8Configuration, JavaScriptEngineSwitcher.V8" />
		</sectionGroup>
		<!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
		<section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
	</configSections>
	<jsEngineSwitcher xmlns="http://tempuri.org/JavaScriptEngineSwitcher.Configuration.xsd">
		<core>
			<engines>
				<add name="V8JsEngine" type="JavaScriptEngineSwitcher.V8.V8JsEngine, JavaScriptEngineSwitcher.V8" />
			</engines>
		</core>
	</jsEngineSwitcher>
	<entityFramework>
		<defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
			<parameters>
				<parameter value="v11.0" />
			</parameters>
		</defaultConnectionFactory>
		<providers>
			<provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
		</providers>
	</entityFramework>
	<appSettings>
		<!--模板的保存根目录，绝对路径，结尾加上\-->
		<add key="SkinBaseDirectory" value="C:\yueyasoft\yycms.v3\WebApplication1\yycms.admin\skin\" />
		<!--消息服务器地址-->
		<add key="MessageQueueServer" value=".\private$\" />
		<!--服务的名称-->
		<add key="ServiceName" value="yueyaCms.Service" />
		<!--后台网址，结尾不要带“/”-->
		<add key="AdminSiteUrl" value="http://localhost:9130" />
		<!--Signal-->
		<add key="signal" value="signalr" />
		<!--CompileEvent-->
		<add key="CompileEvent" value="CompileEvent" />
		<!--GlobalHub-->
		<add key="GlobalHub" value="GlobalHub" />
		<!--后台图片文件夹所属的上级目录，结尾不要带“/”-->
		<add key="AdminImagesPath" value="E:\yueyasoft\yycms.v3\WebApplication1\yycms.admin" />
	</appSettings>
	<connectionStrings>
		<add name="DBConnection" connectionString="metadata=res://*/DBModel.csdl|res://*/DBModel.ssdl|res://*/DBModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=.\sql12;initial catalog=yyCms;persist security info=True;user id=saa;password=123456;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
	</connectionStrings>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-7.0.0.0" newVersion="7.0.0.0" />
			</dependentAssembly>
		</assemblyBinding>
	</runtime>
</configuration>