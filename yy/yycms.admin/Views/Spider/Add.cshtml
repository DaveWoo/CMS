                           id="Title"
                           ng-model="Entity.Title"
                           ng-pattern="/^.{4,12}$/"
                           required class="form-control" />
                    <span class="help-block">蜘蛛名称，由4至12字组成。</span>
                    <span ng-if="myForm.Title.$valid&&!myForm.Title.$pristine" class="glyphicon glyphicon-ok form-control-feedback"></span>
